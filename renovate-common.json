{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "kubernetes": {
    "fileMatch": [
      ".+\\.yaml$"
    ],
    "ignorePaths": [
      "**/vendor/**"
    ]
  },
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePrefixes": [
        "ghcr.io/kgtkr/"
      ],
      "versioning": "regex:^(?<minor>\\d{17})-[0-9a-f]{7}(-(?<compatibility>.*))?$"
    }
  ],
  "major": {
    "enabled": false
  }
}
